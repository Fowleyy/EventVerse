function callUpdateFallAchievment(p: player):
    if {parkour-falls::%{_p}%} is more than 68:
        if {ach.69.%{_p}%} is not set:
            set {ach.69.%{_p}%} to true
            play "LEVEL_UP" to {_p}
            message "&d&l» ACHIEVMENT ODEMČEN: &e69" to {_p}
            message "&7&ohahaha funni number!" to {_p}
            add 1 to {achievements::%{_p}%}
            add 5 to {shards::%{_p}%}

function trapdoorParkourAchievement(p: player):
    if {_p} is in world "parkour2":
        if {ach.trapdoor.%{_p}%} is not set:
            set {ach.trapdoor.%{_p}%} to true
            play "LEVEL_UP" to {_p}
            send "&d&l» ACHIEVMENT ODEMČEN: &eObechcávač" to {_p}
            send "&7&oPokus se otevřít trapdoor na parkour eventu." to {_p}
            add 1 to {achievements::%{_p}%}
            add 3 to {shards::%{_p}%}

