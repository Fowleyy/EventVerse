    cancel event
    if {ach.anvil.%player%} is not set:
        set {ach.anvil.%player%} to true
        play "LEVEL_UP" to player
        send "&d&l» ACHIEVMENT ODEMČEN: &e<Vložte vtip of anvilkách sem>"
        send "&7&ouh.. došly vtipy.. fakt nevim"
        add 1 to {achievements::%player%}
        add 2 to {shards::%player%}


command /sus:
    trigger:
        play song file "plugins/songs/sus.nbs" to player
        send player title "&bඞ" with subtitle "&r " for 3 seconds
        if {ach.sus.%player%} is not set:
            set {ach.sus.%player%} to true
            play "LEVEL_UP" to player
            send "&d&l» ACHIEVMENT ODEMČEN: &esus"
            send "&7&oඞ amungus ඞ"
            add 1 to {achievements::%player%}
            add 5 to {shards::%player%}



